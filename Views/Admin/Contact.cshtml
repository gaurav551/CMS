@model List<Contact>

<h3>@Model.Count Contact </h3>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Email</th>
      <th scope="col">Email</th>
      <th scope="col">Subject</th>
      <th scope="col">Company</th>
      <th colspan="">Message</th>
      <th colspan="2">Action</th>
    </tr> 
  </thead>
  <tbody>
      @foreach (var item in Model)
      {
          
      
    <tr>
      <th scope="row">@item.Email</th>
      <td>@item.Email</td>
      <td>@item.Subject</td>
       <td></td>
      <td>@item.Message</td>
    
     
      
       <td><a asp-action="View" asp-controller="Admin" asp-route-id="@item.Id">View</a></td>
       <td><a asp-action="Delete" asp-controller="Admin" asp-route-id="@item.Id">Delete</a></td>
      


    </tr>
      }
    </tbody>
</table>



 